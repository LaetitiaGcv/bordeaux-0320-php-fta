{{ form_start(form) }}
<div class="row pl pr mt">
    <div class="col s10 offset-s1 pl pr mb">
        {{ form_widget(form.givenName, {'attr': {'value': user.givenName }}) }}
    </div>
</div>
<div class="row pl pr mb-l">
    <div class="col s10 offset-s1 pl pr mb">
        {{ form_widget(form.familyName, {'attr': {'value': user.familyName }}) }}
    </div>
</div>
<div class="row pl pr mb-l">
    <div class="col s10 offset-s1 pl pr mb">
        {{ form_widget(form.email, {'attr': {'value': user.email }}) }}
    </div>
</div>
<div class="row pl pr mb-l">
    <div class="col s10 offset-s1 pl pr mb">
        {{ form_widget(form.telephone, {'attr': {'value': user.telephone }}) }}
    </div>
</div>
<div class="center-align mb">
    <button class="btn"><i class="material-icons">arrow_forward</i></button>
</div>
{{ form_end(form) }}
